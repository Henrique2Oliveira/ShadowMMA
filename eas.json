{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "android": {
        "buildType": "apk",
        "developmentClient": true
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "enableProguardInReleaseBuilds": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
